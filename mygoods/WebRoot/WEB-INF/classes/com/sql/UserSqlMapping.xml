    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.UserDao">
    <!-- 这里namespace必须是Dao接口的路径” -->
   
    <select id="getuser" parameterType="users" resultType="users">
